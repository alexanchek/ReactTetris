    "eslint-plugin-prettier": "4.2.1",
    "lint-staged": "10.0.8",
    "prettier": "2.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.56.2"
  },
  "devDependencies": {
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/parser": "5.46.1",
    "@vitejs/plugin-react-swc": "3.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.31.11",
    "husky": "7.0.0",
    "typescript": "4.9.3",
    "vite": "4.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts,js,tsx,jsx}": "eslint --cache --fix",
